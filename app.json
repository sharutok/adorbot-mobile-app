{
  "expo": {
    "developmentClient": {
      "disableMenuGesture": true
    },
    "name": "Adorbot",
    "slug": "Adorbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Ador_Welding_logo_STAR.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Ador_Welding_logo_STAR.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Ador_Welding_logo_STAR.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.adorbot_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c075057e-c2c5-4bca-ae59-e2bae8415c23"
      }
    }
  }
}
